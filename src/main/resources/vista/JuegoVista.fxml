<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="proyecto.controlador.JuegoControlador">
   <bottom>
      <VBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" spacing="10.0">
               <children>
                  <Button fx:id="btnInicio" mnemonicParsing="false" onAction="#switchToInicio" prefHeight="20.0" prefWidth="50.0" text="Inicio" />
                  <Button fx:id="btnSalir" mnemonicParsing="false" onAction="#salir" prefHeight="20.0" prefWidth="50.0" text="Salir" />
               </children>
               <padding>
                  <Insets bottom="8.0" left="6.0" right="6.0" top="8.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </bottom>
</BorderPane>
